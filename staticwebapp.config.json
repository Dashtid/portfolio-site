{
  "routes": [
    {
      "route": "/api/*",
      "serve": "/api/index.py"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/media/*", "*.{css,scss,js,png,gif,ico,jpg,svg}"]
  }
}
