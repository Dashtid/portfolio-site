{
  "extends": ["development"],
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "hints": {
    "no-inline-styles": "off",
    "compat-api/html": ["error", {
      "ignore": [
        "meta[name=theme-color]",
        "link[fetchpriority]"
      ]
    }]
  },
  "ignoredUrls": [
    {
      "domain": ".*",
      "hints": ["*"]
    }
  ],
  "ignorePatterns": [
    "**/htmlcov/**",
    "**/node_modules/**",
    "**/.coverage/**",
    "**/dist/**",
    "**/.test-results/**",
    "**/.playwright-report/**"
  ]
}
