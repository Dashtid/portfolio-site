# API Configuration
VITE_API_URL=http://localhost:8000

# Application
VITE_APP_VERSION=1.0.0

# Analytics (Privacy-compliant - Plausible or Umami)
VITE_ANALYTICS_ENABLED=true
VITE_ANALYTICS_PROVIDER=plausible  # 'plausible' or 'umami'
VITE_ANALYTICS_SITE_ID=dashti.se  # Your site domain or Umami website ID
VITE_ANALYTICS_URL=https://plausible.io/js/script.js  # Leave empty for default

# Error Tracking (Sentry)
VITE_ERROR_TRACKING_ENABLED=true
VITE_SENTRY_DSN=""  # Sentry DSN for frontend error tracking
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1  # 10% of transactions for performance monitoring

# Performance Monitoring
VITE_METRICS_ENABLED=true
