{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --config vite.config.production.js",
    "preview": "vite preview",
    "preview:prod": "vite preview --config vite.config.production.js",
    "test": "vitest",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:visual": "playwright test visual.spec.ts --project=chromium",
    "test:e2e:visual:update": "playwright test visual.spec.ts --project=chromium --update-snapshots",
    "test:e2e:docker": "docker run --rm --ipc=host -v \"$(pwd):/app\" -w /app mcr.microsoft.com/playwright:v1.57.0-noble npx playwright test",
    "test:e2e:visual:docker": "docker run --rm --ipc=host -v \"$(pwd):/app\" -w /app mcr.microsoft.com/playwright:v1.57.0-noble npx playwright test visual.spec.ts --project=chromium",
    "test:e2e:visual:update-docker": "docker run --rm --ipc=host -v \"$(pwd):/app\" -w /app mcr.microsoft.com/playwright:v1.57.0-noble npx playwright test visual.spec.ts --project=chromium --update-snapshots",
    "lighthouse": "lhci autorun --config=../.github/lighthouse/lighthouserc.json",
    "lighthouse:collect": "lhci collect --config=../.github/lighthouse/lighthouserc.json",
    "coverage": "vitest run --coverage",
    "analyze": "vite build --config vite.config.production.js && start dist/stats.html",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "prepare": "husky || true",
    "images:avif": "node scripts/convert-to-avif.mjs"
  },
  "dependencies": {
    "@sentry/vue": "^8.42.0",
    "@types/three": "^0.182.0",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "dompurify": "~3.3.0",
    "gsap": "^3.14.2",
    "pinia": "^3.0.4",
    "three": "^0.182.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.5"
  },
  "overrides": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.49.0",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.14",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.7.0",
    "happy-dom": "^20.3.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.8.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.2.2",
    "workbox-window": "^7.4.0"
  }
}
